{
  "name": "zerion-sdk",
  "version": "0.0.0",
  "description": "A Typed Interface for ZerionAPI",
  "author": "bh2smith",
  "repository": {
    "type": "git",
    "url": "https://github.com/bh2smith/zerion-sdk"
  },
  "bugs": {
    "url": "https://github.com/bh2smith/zerion-sdk/issues"
  },
  "keywords": [
    "ethereum",
    "api",
    "client-sdk",
    "zerion"
  ],
  "license": "MIT",
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "generate-static-data": "tsx scripts/generate-static-data.ts",
    "build": "rm -fr dist/* && bun build:esm && bun build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev": "tsx src/index.ts",
    "lint": "eslint . --ignore-pattern dist/ && prettier --check '{src,examples,tests,scripts}/**/*.ts'",
    "fmt": "prettier --write '{src,examples,tests,scripts}/**/*.ts' && eslint . --ignore-pattern dist/ --fix"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "viem": "^2.37.8"
  }
}
